var localVault = FairnetVault.all.first();
var remoteVaults = localVault.remoteVaults.values();

var allPosts = Sequence{};

var post = remoteVaults.first().posts.second();
"post: ".print(); post.println();

var condition = true;
var endString1 = "0";
var endString2 = "1";

var allVaults = Sequence{};
allVaults.addAll(remoteVaults);
allVaults.add(localVault);
allPosts = allVaults.parallelCollect(
	rv| rv.posts.parallelSelect(p | p.isPublic = condition
		and p.content.endsWith(concatenate(endString1, endString2))
		)
	).flatten();

return allPosts;

operation concatenate(a: String, b: String): String {
	return a + b;
}




			