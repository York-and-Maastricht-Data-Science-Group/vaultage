<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="85.0" prefWidth="899.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="labelFriendUsername" layoutX="100.0" layoutY="29.0" text="Felis Domestica">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="labelFriendStatus" alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="680.0" layoutY="29.0" text="Requested" textAlignment="RIGHT" AnchorPane.rightAnchor="134.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ImageView fx:id="imageViewFriend" fitHeight="75.0" fitWidth="75.0" layoutX="5.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/profile_75x75.png" />
         </image>
      </ImageView>
      <Button fx:id="buttonFriendRemove" layoutX="780.0" layoutY="23.0" mnemonicParsing="false" onAction="#buttonFriendRemoveOnAction" prefHeight="0.0" prefWidth="105.0" text="Remove" AnchorPane.rightAnchor="14.0">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
